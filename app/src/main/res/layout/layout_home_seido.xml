<?xml version="1.0" encoding="utf-8"?>
<com.fa.google.shopassist.cards.SeidoCardHome
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    >

    <android.support.v7.widget.CardView
        style="@style/Card"
        app:cardCornerRadius="3dp"
        android:id="@+id/card_home_nexus6"
        android:transitionName="card_nexus_6"
        android:layout_width="wrap_content"
        android:layout_marginLeft="8dp"
        android:foreground="?android:attr/selectableItemBackground"
        >
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/home_5"
            android:id="@+id/img_nexus_home_bg"
            />

        <ImageView
            android:layout_width="111dp"
            android:layout_height="96dp"
            android:id="@+id/img_nexus6_home"
            android:scaleType="fitXY"
            android:clickable="false"
            android:focusable="true"
            android:transitionName="nexus6_image"
            android:layout_marginLeft="233dp"

            />


    </android.support.v7.widget.CardView>


</com.fa.google.shopassist.cards.SeidoCardHome>