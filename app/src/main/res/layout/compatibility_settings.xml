<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    >

    <RelativeLayout style="@style/ToggleRow">
        <TextView style="@style/ToggleText"
            android:text="In-store browsing history"
            />
        <ToggleButton style="@style/ToggleButton"
            android:textOff=""
            android:textOn=""
            android:button="@drawable/toggle_button"
            android:checked="true"
            />
    </RelativeLayout>
    <LinearLayout style="@style/Divider" />

    <RelativeLayout style="@style/ToggleRow">
        <TextView style="@style/ToggleText"
            android:text="Recent searches"
            />
        <ToggleButton style="@style/ToggleButton"
            android:textOff=""
            android:textOn=""
            android:button="@drawable/toggle_button"
            android:checked="true"
            />
    </RelativeLayout>
    <LinearLayout style="@style/Divider" />

    <RelativeLayout style="@style/ToggleRow">
        <TextView style="@style/ToggleText"
            android:text="Recent purchases"
            />
        <ToggleButton style="@style/ToggleButton"
            android:textOff=""
            android:textOn=""
            android:button="@drawable/toggle_button"
            />
    </RelativeLayout>
    <LinearLayout style="@style/Divider" />

    <RelativeLayout style="@style/ToggleRow">
        <TextView style="@style/ToggleText"
            android:text="Lists I follow"
            />
        <ToggleButton style="@style/ToggleButton"
            android:textOff=""
            android:textOn=""
            android:button="@drawable/toggle_button"
            android:checked="true"
            />
    </RelativeLayout>
    <LinearLayout style="@style/Divider" />

    <RelativeLayout style="@style/ToggleRow">
        <TextView style="@style/ToggleText"
            android:text="Reviews I left"
            />
        <ToggleButton style="@style/ToggleButton"
            android:textOff=""
            android:textOn=""
            android:button="@drawable/toggle_button"
            />
    </RelativeLayout>
    <LinearLayout style="@style/Divider" />

    <RelativeLayout style="@style/ToggleRow">
        <TextView style="@style/ToggleText"
            android:text="App usage"
            />
        <ToggleButton style="@style/ToggleButton"
            android:textOff=""
            android:textOn=""
            android:button="@drawable/toggle_button"
            android:checked="true"
            />
    </RelativeLayout>
    <LinearLayout style="@style/Divider" />

    <RelativeLayout style="@style/ToggleRow">
        <TextView style="@style/ToggleText"
            android:text="Shop Assist preferences"
            />
        <ToggleButton style="@style/ToggleButton"
            android:textOff=""
            android:textOn=""
            android:button="@drawable/toggle_button"
            />
    </RelativeLayout>

</LinearLayout>